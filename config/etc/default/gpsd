# GPS CLOCK -  PPS signals

START_DAEMON="true"
GPSD_OPTIONS="-n"
DEVICES="/dev/ttyS0 /dev/pps0"
USBAUTO="false"
GPSD_SOCKET="/var/run/gpsd.sock"
